{
  "version": 1,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "proxy": [
    {
      "source": "/v2/top-headlines",
      "destination": "https://newsapi.org"
    },
    {
      "source": "/v1/(.*)",
      "destination": "https://api.updatecome.com:8081"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://opendart.fss.or.kr"
    }
  ]
}

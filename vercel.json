{
  "version": 2,
  "routes": [
    {
      "src": "/v1/(.*)",
      "dest": "http://api.updatecome.com:8081/",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "https://opendart.fss.or.kr/",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET"
      }
    },
    {
      "src": "/v2/(.*)",
      "dest": "https://newsapi.org/",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET"
      }
    }
  ]
}
